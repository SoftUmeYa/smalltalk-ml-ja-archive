Object subclass: #TwoPTextEditer	instanceVariableNames: 'window color label text1 text2'	classVariableNames: ''	poolDictionaries: ''	category: 'SCL-Tool'!!TwoPTextEditer methodsFor: 'accessing' stamp: 'sy 6/1/2007 12:35'!color  ^ window color! !!TwoPTextEditer methodsFor: 'accessing' stamp: 'sy 6/1/2007 12:30'!label  ^window label.! !!TwoPTextEditer methodsFor: 'accessing' stamp: 'sy 6/1/2007 12:49'!setColor:anObject  color := anObject.  window setWindowColor:color.! !!TwoPTextEditer methodsFor: 'accessing' stamp: 'sy 6/1/2007 12:31'!setLabel:aString  label:=aString.  window setLabel:label.! !!TwoPTextEditer methodsFor: 'accessing' stamp: 'sy 6/1/2007 12:56'!setText1:aString  | t1 |  t1 :=(window submorphs ) at:2 .  t1 setText:aString.! !!TwoPTextEditer methodsFor: 'accessing' stamp: 'sy 6/1/2007 12:56'!setText2:aString  | t2 |  t2 :=(window submorphs ) at:3 .  t2 setText:aString.! !!TwoPTextEditer methodsFor: 'accessing' stamp: 'sy 6/1/2007 12:45'!text1  | t1 |  t1 :=(window submorphs ) at:2 .  ^t1 text.! !!TwoPTextEditer methodsFor: 'accessing' stamp: 'sy 6/1/2007 12:46'!text2  | t2 |  t2 :=(window submorphs ) at:3 .  ^t2 text.! !!TwoPTextEditer methodsFor: 'initialize-release' stamp: 'sy 6/13/2007 10:25'!initialize  | textMorph1 textMorph2 |  color := Color red.  label :='Two Pane Text Editor'.  window := SystemWindow labelled:label.  window setWindowColor: color.  textMorph1 := PluggableTextMorph 	on: Workspace new				text: #contents  	accept: #acceptContents:  	readSelection: nil  	menu: #codePaneMenu:shifted:.  textMorph2 := PluggableTextMorph 	on: Workspace new				text: #contents  	accept: #acceptContents:  	readSelection: nil  	menu: #codePaneMenu:shifted:.  window addMorph: textMorph1 frame: (0@0 corner: 1@0.5).  window addMorph: textMorph2 frame: (0@0.5 corner: 1@1).  window openInWorld.! !!TwoPTextEditer methodsFor: 'comment' stamp: 'sy 10/28/2007 22:34'!comment  ^'このアプリケーションは開発途中です'!]lang[(12 17 1)0,5,0! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!TwoPTextEditer class	instanceVariableNames: ''!!TwoPTextEditer class methodsFor: 'initialize-release' stamp: 'sy 6/1/2007 13:12'!open  ^self openEditer.! !!TwoPTextEditer class methodsFor: 'initialize-release' stamp: 'sy 6/1/2007 13:11'!openEditer  ^self new.! !